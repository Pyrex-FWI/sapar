{
    "name": "pyrex-fwi/sapar-id3",
    "description": "The package lays the basis for the simple manipulation of ID3 tags.\nYou can define readers and writers to manipulate the metadata in read or write.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Pyrex Fwi",
            "email": "pyrex-fwi@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sapar\\Component\\Id3\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": "^7.3",
        "pyrex-fwi/core-contracts": "^5.1.4",
        "symfony/console": "4.* || 5.*",
        "symfony/process": "4.* || 5.*",
        "symfony/stopwatch": "4.* || 5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpro/grumphp": "^0.18",
        "symfony/var-dumper": "^4.3 | ^5.1",
        "phpmetrics/phpmetrics": "^2.6"
    }
}
