{
    "name": "pyrex-fwi/sapar",
    "require": {
        "php": "^7.3",
        "doctrine/collections": "^1.5",
        "doctrine/orm": "*",
        "fza/mysql-doctrine-levenshtein-function": "^0.3.0",
        "gedmo/doctrine-extensions": "^2.4",
        "psr/log": "^1.1",
        "symfony/config": "^4.3",
        "symfony/console": "4.* || 5.*",
        "symfony/dotenv": "^4.3",
        "symfony/expression-language": "^4.3",
        "symfony/filesystem": "^4.3",
        "symfony/finder": "^4.3",
        "symfony/process": "4.* || 5.*",
        "symfony/serializer": "^4.0 || ^5.0",
        "symfony/stopwatch": "4.* || 5.*",
        "symfony/validator": "^4.0 || ^5.0",
        "symfony/yaml": "*",
        "symplify/monorepo-builder": "^7.2"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.11",
        "mikey179/vfsstream": "^1.6",
        "phpmetrics/phpmetrics": "^2.6",
        "phpunit/phpunit": "^9.1",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "classmap": [
            "Component/audio-core-entities/./tests/",
            "Component/audio-core-entities/src/"
        ],
        "exclude-from-classmap": [
            "Console/ReleaseDispatcher/src/Tests/",
            "Console/ReleaseDispatcher/tests/"
        ],
        "psr-0": {
            "AudioCoreEntity": "Component/audio-core-entities/./src/",
            "AudioCoreEntity\\Tests": "Component/audio-core-entities/./tests/",
            "SymfonyStandard": "Component/audio-core-entities/app/"
        },
        "psr-4": {
            "Sapar\\Component\\Id3\\": "Component/id3/src",
            "Sapar\\Component\\Id3\\Test\\": "Component/id3/Test",
            "Sapar\\Component\\SfvChecker\\": "Component/SfvChecker/src",
            "Sapar\\Console\\MovieUnrar\\": "Console/MovieUnrar/src",
            "Sapar\\Console\\ReleaseDispatcher\\": "Console/ReleaseDispatcher/src",
            "Sapar\\Console\\ReleaseDispatcher\\Tests\\": "Console/ReleaseDispatcher/tests",
            "Sapar\\Mfi\\": "Component/MediaFileIndexer/./",
            "Sapar\\Mfo\\": "Console/sapar-mfo/src",
            "Sapar\\Mfo\\Test\\": "Console/sapar-mfo/Test"
        }
    },
    "replace": {
        "pyrex-fwi/audio-core-entities": "self.version",
        "pyrex-fwi/sapar-id3": "self.version",
        "pyrex-fwi/sapar-mfi": "self.version",
        "pyrex-fwi/sapar-mfo": "self.version",
        "sapar/movie-unrar": "self.version",
        "sapar/release-dispatcher": "self.version",
        "sapar/sfv-checker": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
