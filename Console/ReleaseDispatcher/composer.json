{
    "name": "sapar/release-dispatcher",
    "description": "Lib for unrar movies on nas",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pyrex-FWI",
            "email": "yemistikrys@gmail.com"
        }
